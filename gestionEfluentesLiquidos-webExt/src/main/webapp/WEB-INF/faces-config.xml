<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>mbCude</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.aplicaciones.gestionefluentesliquidos.webext.mb.MbCude</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbRegistro</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.aplicaciones.gestionefluentesliquidos.webext.mb.MbRegistro</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbSesion</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.aplicaciones.gestionefluentesliquidos.webext.mb.MbSesion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>sesionPrimera</from-outcome>
            <to-view-id>/sesion/iniciadoPrimeraVez.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>sesionContinua</from-outcome>
            <to-view-id>/sesion/iniciado.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
